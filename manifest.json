{
    "manifest_version": 2,
  
    "name": "NewComics",
    "description": "This extension allows the user to add comics from readcomiconline.to and notifies if new Issues are added. If the user cookies for the page has expired, it navigates to the page first.",
    "version": "1.0",

    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png"
      },
      "default_popup": "popup.html",
      "default_title": "NewComics"
    },
    "permissions": [
      "activeTab",
      "storage",
      "http://readcomiconline.to/*",
      "https://readcomiconline.to/*",
      "cookies"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }